def pascal(n):
    k=1
    for i in range(1,n+1):
        lst=[]
        for j in range(0,i):
            if j==0 or i==0:
                k=1
            else:
                k=k*(i-j)//j
            lst.append(k)
        print(lst)
    print()
i= int(input("num: ") #enter number of rows
pascal(i) #calling pascal fynction with parameter i
